<div>
  <%= form_tag '/zombies', method: :post do %>
    <div>
      <%= label_tag :name %>
      <%= text_field_tag :name %>
    </div>
    <div>
      <%= label_tag :type %>
      <%= text_field_tag :type %>
    </div>
    <div>
      <%= label_tag :weakness %>
      <%= text_field_tag :weakness %>
    </div>
    <div>
      <%= label_tag :region %>
      <%= text_field_tag :region %>
    </div>
    <div>
      <%= label_tag :speed %>
      <%= text_field_tag :speed %>
    </div>


    
  <% end %>
</div>